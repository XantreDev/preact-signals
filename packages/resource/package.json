{
  "name": "@preact-signals/resource",
  "version": "0.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Utils for @preact/signals",
  "sideEffects": false,
  "repository": {
    "url": "https://github.com/XantreGodlike/preact-signals-utils",
    "type": "git"
  },
  "author": {
    "email": "valerik007.vs@gmail.com",
    "name": "Valerii Smirnov",
    "url": "https://github.com/XantreGodlike"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "source": "./src/index.ts",
  "react-native": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "main": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:esm": "tsc --outDir ./dist/esm",
    "build:cjs": "tsc --module commonjs --outDir ./dist/cjs",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "watch": "concurrently \"pnpm build:esm -w\" \"pnpm build:cjs -w\"",
    "build": "pnpm clean && concurrently \"pnpm build:esm\" \"pnpm build:cjs\""
  },
  "peerDependencies": {
    "@preact/signals-react": ">=1.2.0",
    "@preact/signals-core": ">=1.2.0",
    "react": "17.*.* || 18.*.*"
  },
  "peerDependenciesMeta": {
    "@preact/signals-react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@preact/signals-core": "^1.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.0",
    "type-fest": "^3.12.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@preact-signals/utils": "workspace:*"
  },
  "keywords": [],
  "license": "MIT"
}
