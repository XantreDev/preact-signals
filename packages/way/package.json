{
  "name": "@one-render/way",
  "version": "0.0.4",
  "description": "Simple hoc for one render of components",
  "type": "module",
  "main": "./dist/way.js",
  "module": "./dist/way.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/way.js",
      "require": "./dist/way.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "source": "./src/index.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:way": "microbundle --raw",
    "build": "pnpm clean && pnpm build:way",
    "prepare": "ts-patch install -s"
  },
  "dependencies": {
    "radash": "^10.7.0",
    "type-fest": "^3.5.3"
  },
  "peerDependencies": {
    "@preact/signals-react": "^1.2.2",
    "react": "17.*.* || 18.*.*"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "rimraf": "^4.1.2",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "keywords": [],
  "author": "XanterGodlike",
  "license": "MIT"
}
